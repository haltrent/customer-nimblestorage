<?xml version="1.0" encoding="UTF-8" ?>

<project name="bmanifest-extension-client" default="" basedir=".">

	<target name="bmanifest_postprocess_client" extensionOf="bmanifest_postprocess">
		<echostart>bmanifest_postprocess_client</echostart>

		<!-- This is the place to manipulate all the output generated by the build manifest.
		 You can also process any log info and create your own content to be sent 
		 in the notification -->

		 <echoend>bmanifest_postprocess_client</echoend> 
	</target>

	<target name="bmanifest_notify_client" extensionOf="bmanifest_notify_init">
		<echostart>bmanifest_notify_client</echostart>

		<!-- you can change which smtp.properties are loaded by creating your own
		 smtp.properties
		 You can create your temporary smtp.properties in the outgen.job.dir
		 or create a permanent template in ${outgen.resources.dir.client} 
		 You can include properties in your file, they will be expanded at load time -->

		<!--<property name="bmanifest.smtp.property.filename" localtion="${outgen.resources.dir.client}/buildmanifest.smtp.properties"/>-->

		<echoend>bmanifest_notify_client</echoend> 
	</target>


</project>